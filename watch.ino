void setup() 
{
  Serial.begin(9600);  
  u8g.setFont(u8g_font_unifont);
  pinMode(button,INPUT);
  pinMode(next,INPUT);
  pinMode(blue,OUTPUT);
 pinMode(fwd,INPUT);
  pinMode(buzz,OUTPUT);
 pinMode(back,INPUT);
   
  cli();
  
  TCCR1A = 0;
  TCCR1B = 0;
  TCNT1  = 0;
  OCR1A = 15624;
  TCCR1B |= (1 << WGM12);
  TCCR1B |= (1 << CS12) | (1 << CS10);  
  TIMSK1 |= (1 << OCIE1A);
  sei();
}

  
